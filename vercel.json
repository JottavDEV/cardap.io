{
    "version": 2,
    "buildCommand": "npx expo export --platform web",
    "outputDirectory": "dist",
    "devCommand": "npx expo start --web",
    "installCommand": "npm install",
    "framework": null,
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/:path*",
        "destination": "/"
      }
    ],
    "headers": [
      {
        "source": "/_expo/static/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/(.*\\.js)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/(.*\\.css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/(.*\\.(woff|woff2|ttf|eot))",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/(.*\\.(svg|png|jpg|jpeg|gif|ico|webp))",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }